    "# This section trains a model and pickles it\n",
    "import pandas as pd\n",
    "from sklearn.tree import DecisionTreeClassifier\n",
    "from sklearn.preprocessing import LabelEncoder\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.metrics import accuracy_score\n",
    "import joblib\n",
    "\n",
    "def train_and_test_model():\n",
    "    train_data = pd.read_csv('TrainData.csv')\n",
    "    input = train_data.drop(columns=['Order'])\n",
    "    output = train_data['Order']\n",
    "\n",
    "    encoder = LabelEncoder()\n",
    "    input['Year'] = encoder.fit_transform(input['Year'])\n",
    "    input['Major'] = encoder.fit_transform(input['Major'])\n",
    "    input['University'] = encoder.fit_transform(input['University'])\n",
    "\n",
    "    input_train, input_test, output_train, output_test = train_test_split(input, output, test_size=0.15)\n",
    "\n",
    "    model = DecisionTreeClassifier()\n",
    "    model.fit(input_train, output_train)\n",
    "    predictions = model.predict(input_test)\n",
    "\n",
    "    accuracy = f\"{str(accuracy_score(output_test, predictions)*100)}%\"\n",
    "    model_file_name = 'foodx-prediction-model.joblib'\n",
    "    joblib.dump(model, model_file_name)\n",
    "    print(f\"Model with {accuracy} accuracy saved as {model_file_name}\")\n",
    "\n",
    "    model_file_name = \"foodx-prediction-model.joblib\"\n",
    "    model = joblib.load(model_file_name)\n",
    "\n",
    "    # The following line is an encoding of Year 3, Mathematics, Indiana State University, 12\n",
    "    student = [2, 12, 0, 12]\n",
    "    predictions = model.predict([student])\n",
    "    print(predictions)"
   ]
